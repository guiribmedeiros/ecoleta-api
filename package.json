{
  "name": "ecoleta-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "node build/bootstrap/app.ts",
    "serve": "ts-node-dev bootstrap/app.ts --transpile-only --ignore-watch node_modules --respawn",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "db:seed": "knex seed:run --knexfile knexfile.ts",
    "build": "tsc"
  },
  "license": "MIT",
  "dependencies": {
    "celebrate": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "multer": "^1.4.2",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/multer": "^1.4.5",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
